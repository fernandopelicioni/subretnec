  object QrGrupos: TFDQuery
    Connection = Pai.FdCorpore
    SQL.Strings = (
      'select '
      ''
      'IDPRD,'
      'CODIGOPRD,'
      'NOMEFANTASIA '
      ''
      ' from TPRD '
      ''
      'WHERE ULTIMONIVEL = 0'
      '  AND CODCOLIGADA =:COLIGADA '
      
        '  AND CODIGOPRD IN ('#39'001'#39', '#39'002'#39','#39'003'#39','#39'004'#39','#39'005'#39','#39'006'#39','#39'007'#39','#39 +
        '008'#39','#39'009'#39','#39'010'#39','#39'011'#39','#39'013'#39') '
      ''
      'ORDER BY CODIGOPRD')
    Left = 24
    Top = 9
    ParamData = <
      item
        Name = 'COLIGADA'
        DataType = ftString
        ParamType = ptInput
        Value = '1'
      end>
  end
  object DTGRUPOS: TDataSource
    DataSet = QrGrupos
    Left = 104
    Top = 8
  end
  object QrSubGrupo: TFDQuery
    Connection = Pai.FdCorpore
    SQL.Strings = (
      'select '
      'A.IDPRD,'
      'A.NOMEFANTASIA,'
      'A.CODIGOPRD'
      'from TPRD A'
      ''
      
        'LEFT JOIN TPRD B ON B.IDPRD = A.IDPRD AND B.CODCOLIGADA = A.CODC' +
        'OLIGADA AND CONVERT(VARCHAR(7),B.CODIGOPRD) = A.CODIGOPRD AND B.' +
        'ULTIMONIVEL = '#39'0'#39
      ''
      'WHERE A.ULTIMONIVEL = 0'
      '  AND A.CODCOLIGADA = 1'
      '  AND CONVERT(VARCHAR(3),A.CODIGOPRD) = (:grupo)'
      ''
      'ORDER BY A.CODIGOPRD'
      '')
    Left = 168
    Top = 9
    ParamData = <
      item
        Name = 'GRUPO'
        ParamType = ptInput
      end>
  end
  object DTSUBGRUPO: TDataSource
    DataSet = QrSubGrupo
    Left = 232
    Top = 16
  end
