  object QrAvarias: TFDQuery
    Connection = pai.FDConectGenoma
    SQL.Strings = (
      'Select '
      'AVMOV_VEI,'
      'AVMOV_DATA,'
      'AVMOV_HIS,'
      ''
      'CASE WHEN  MOV.AVMOV_RESP_TIPO = '#39'C'#39' THEN '#39'COLABORADOR'#39
      '     WHEN  MOV.AVMOV_RESP_TIPO = '#39'V'#39' THEN '#39'VANDALISMO'#39
      '     WHEN  MOV.AVMOV_RESP_TIPO = '#39'T'#39' THEN '#39'TERCEIROS'#39
      '     WHEN  MOV.AVMOV_RESP_TIPO = '#39'N'#39' THEN '#39'N'#195'O IDENTIFICADO'#39
      'END AS TIPORESP,'
      'MOV.AVMOV_RESP_CHAPA,'
      'Mov.AVMOV_RESP_NOME,'
      'B.TAV_DESCRICAO as Avaria,'
      ''
      'Mov.AVMOV_POSX,'
      'Mov.AVMOV_POSY'
      ''
      ''
      ' from tb_AvaMov Mov'
      ''
      'Join TB_AVA_ITENS B on B.TAV_ID = Mov.AVMOV_TAV'
      ''
      'where avmov_vei=:veiculo'
      'and avmov_col=:coligada'
      'and avMov_status='#39'A'#39)
    Left = 288
    Top = 224
    ParamData = <
      item
        Name = 'VEICULO'
        DataType = ftString
        ParamType = ptInput
        Value = '21100'
      end
      item
        Name = 'COLIGADA'
        DataType = ftString
        ParamType = ptInput
        Value = '1'
      end>
  end
